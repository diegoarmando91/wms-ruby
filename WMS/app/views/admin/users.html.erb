<h1>Warehouse Users</h1>
	<% if @users.empty?	%>
		<p>No users have been created for this user.</p>
	<% else 	%>
		<table>
			<tr><th>Full Name	</th>	<th>Username	</th>	<th>Email	</th>	</tr>
			<% for user in @users	%>
				<tr><td><%= user.full_name																		%>	</td>
					<td><%= user.username																		%>	</td>
					<td><%= user.email																			%>	</td>
					<td><%= link_to "View", :controller => 'admin', :action => 'view_user', :user => user		%>	</td>
					<td><%= link_to "Edit", :controller => 'admin', :action => 'edit_user', :user => user		%>	</td>
				</tr>
			<% end %>
		</table>
	<% end %>
	<br><br><hr><br>
	<% form_for :user, :url => {:controller => 'admin', :action => 'create_user'} do |form|	%>
		<%= form.hidden_field :warehouse_id, :value => @warehouse.id	%>
		<%= render :partial => 'user', :locals => {:form => form 	}	%>
		<table>
			<tr><td>Password					</td>	<td><%= form.password_field	:password								%>	</td></tr>
			<tr><td>Password Confirmation		</td>	<td><%= form.password_field	:password_confirmation					%>	</td></tr>
		</table>
		<%= submit_tag "Create User"	%>
	<% end %>