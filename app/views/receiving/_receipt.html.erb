<table>
	<tr><td>Supplier				</td>	<td><%= form.select :supplier_id, @warehouse.company.suppliers.collect {|s| [s.name, s.id]}, :include_blank => true				%>	</td></tr>
	<tr><td>Receipt Type			</td>	<td><%= form.select :receipt_type_id, @warehouse.company.receipt_types.collect {|rt| [rt.code, rt.id]}, :include_blank => true	%>	</td></tr>
	<tr><td>Estimated Receipt Date	</td>	<td><%= form.date_select :estimated_receipt_date, :include_blank => true														%>	</td></tr>
	<tr><td>Receipt Number			</td>	<td><%= form.text_field :receipt_number																							%>	</td></tr>
</table>