<table>
	<tr><td>Order Sequence			</td>	<td><%= form.text_field :order_sequence			%>	</td></tr>
	<tr><td>Order Type				</td>	<td><%= form.select :order_type_id, @warehouse.company.order_types.collect {|ot| [ot.order_type, ot.id]}, 				:include_blank => true	%>	</td></tr>
	<tr><td>Product Category		</td>	<td><%= form.select :product_category_id, @warehouse.company.product_categories.collect {|pc| [pc.name, pc.id]}, 		:include_blank => true	%>	</td></tr>
	<tr><td>Product Subcategory		</td>	<td><%= form.select :product_subcategory_id, @warehouse.company.product_subcategories.collect {|ps| [ps.name, ps.id]},	:include_blank => true	%>	</td></tr>
	<tr><td>Customer				</td>	<td><%= form.select :customer_id, @warehouse.company.customers.collect {|c| [c.name, c.id]}, 							:include_blank => true	%>	</td></tr>
	<tr><td>Customer Type			</td>	<td><%= form.select :customer_type_id, @warehouse.company.customer_types.collect {|ct| [ct.code, ct.id]}, 				:include_blank => true	%>	</td></tr>
	<tr><td>Allocation Strategy 	</td>	<td><%= form.select :allocation_strategy_id, @warehouse.allocation_strategies.collect {|as| [as.name, as.id]}									%>	</td></tr>
</table>